{
  "name": "contributte/thepay",
  "description": "ThePay library - nette extension",
  "keywords": [
    "the pay",
    "nette",
    "extension"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1 | ^8.0",
    "ext-soap": "*",
    "contributte/thepay-api": "^4.0.0",
    "nette/di": "^3.0",
    "nette/application": "^3.0",
    "nette/http": "^3.0"
  },
  "require-dev": {
    "nette/bootstrap": "^3.0",
    "tracy/tracy": "^2.5.0",
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.3",
    "phpstan/phpstan": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "phpstan/extension-installer": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\ThePay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/fixtures/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "v4.2.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
